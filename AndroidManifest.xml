<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="no.cbiscuit.games.dslider.activity" android:versionCode="1" android:versionName="1.0-SNAPSHOT">

  <application android:icon="@drawable/icon" android:label="@string/app_name">
    <activity android:name=".StartUpActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".StartMenuActivity">
      <intent-filter>
        <action android:name="no.cbiscuit.games.dslider.activity.STARTMENU" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <activity android:name=".FirstLevelActivity" android:label="@string/app_name"
			android:screenOrientation="portrait">
			<intent-filter>
				<action android:name="no.cbiscuit.games.dslider.activity.FIRSTLEVEL" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
  </application>

</manifest>

